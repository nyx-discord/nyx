{
  "name": "@nyx-discord/core",
  "version": "1.3.0",
  "description": "The core of nyx. Contains the object interfaces and contracts that make up the framework.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyx-discord/nyx.git"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist/**",
    "assets/**"
  ],
  "scripts": {
    "lint": "eslint ./src --fix",
    "format": "prettier --write ./src",
    "barrel": "barrelsby --directory ./src --delete --singleQuotes",
    "build": "tsup-node src/index.ts && tsc --emitDeclarationOnly --declaration --declarationDir ./dist",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js",
    "framework"
  ],
  "author": "Amgelo563",
  "license": "MIT",
  "devDependencies": {
    "@discordjs/collection": "1.5.3"
  },
  "peerDependencies": {
    "discord.js": "^14.14.1"
  }
}
